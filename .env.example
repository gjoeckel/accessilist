# AccessiList Environment Configuration
# Copy this file to .env and configure for your environment

# ENVIRONMENT SELECTION (local, production, staging)
APP_ENV=local

# LOCAL ENVIRONMENT
BASE_PATH_LOCAL=
API_EXT_LOCAL=.php
DEBUG_LOCAL=true

# PRODUCTION ENVIRONMENT  
BASE_PATH_PRODUCTION=/training/online/accessilist
API_EXT_PRODUCTION=
DEBUG_PRODUCTION=false

# STAGING ENVIRONMENT
BASE_PATH_STAGING=/staging/accessilist
API_EXT_STAGING=
DEBUG_STAGING=true
